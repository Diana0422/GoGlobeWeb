<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="50.0" prefWidth="395.0" stylesheets="@../../css/application.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="logic.view.control.dynamic.UserItemGraphic">
   <children>
      <HBox layoutX="-1.0" prefHeight="50.0" prefWidth="400.0">
         <children>
            <Label fx:id="lblUserName" prefHeight="50.0" prefWidth="195.0" text="Name Surname">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
               <padding>
                  <Insets left="10.0" />
               </padding>
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font>
            </Label>
            <Label fx:id="lblAge" alignment="CENTER" prefHeight="53.0" prefWidth="55.0" text="23">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <Button id="btn" fx:id="btnProfile" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#displayProfile" prefHeight="33.0" prefWidth="53.0" text="profile">
               <HBox.margin>
                  <Insets left="50.0" top="8.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
