<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="form" stylesheets="@css/application.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="logic.view.AlertGraphic">
   <children>
      <Pane prefHeight="271.0" prefWidth="444.0">
         <children>
            <Label fx:id="lblAlertMessage" layoutX="55.0" layoutY="14.0" prefHeight="58.0" prefWidth="335.0" text="Participation to trip verified.">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <Label fx:id="lblAlertDescription" alignment="CENTER" layoutX="60.0" layoutY="72.0" prefHeight="58.0" prefWidth="324.0" text="You gained 20 points.">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="btnOption1" layoutX="60.0" layoutY="147.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="134.0" text="Redeem Prize">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="btnOption2" layoutX="270.0" layoutY="147.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="73.0" text="Home">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
